
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h3>All Users in DB</h3>
        <table>
            <tr>
                <td>
                    <p>login</p>
                </td>
                <td>
                    <p>password</p>
                </td>
                <td></td>
            </tr>
            @foreach (var b in ViewBag.Users)
            {
                <tr>
                    <td>
                        <p>@b.Login</p>
                    </td>
                    <td>
                        <p>@b.Password</p>
                    </td>
                </tr>
            }
        </table>
    </div>
    <a href="/Home/Register">Registration</a>
</body>
</html>

